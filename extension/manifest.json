{
  "name": "Pretty Print Gmail",
  "description": "This extension cleans up printing for Gmail",
  "version": "0.9",
  "permissions": [
  "activeTab", "https://mail.google.com/mail/u/0/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
      "default_title": "Pretty print this email!"
  },
  "manifest_version": 2
}